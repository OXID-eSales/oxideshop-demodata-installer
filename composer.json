{
  "name": "oxid-esales/oxideshop-demodata-installer",
  "description": "This tool is used to copy pictures from demo data repository to OXID eShop, during setup process.",
  "license": "GPL-3.0-only",
  "type": "oxideshop-component",
  "require": {
    "php": ">=7.3",
    "symfony/filesystem": "^5.0.4",
    "oxid-esales/oxideshop-facts": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~9",
    "mikey179/vfsstream": "^1.6",
    "webmozart/path-util": ">=2.3",
    "oxid-esales/oxideshop-ce": "dev-master",
    "oxid-esales/oxideshop-unified-namespace-generator": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": { "OxidEsales\\DemoDataInstaller\\Tests\\": "tests/" }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "./bin/oe-eshop-demodata_install"
  ]
}
