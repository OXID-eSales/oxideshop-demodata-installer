{
  "name": "oxid-esales/oxideshop-demodata-installer",
  "description": "This tool is used to copy pictures from demo data repository to OXID eShop, during setup process.",
  "license": "GPL-3.0-only",
  "type": "oxideshop-component",
  "require": {
    "php": ">=8.0",
    "oxid-esales/oxideshop-facts": "*",
    "symfony/filesystem": "*"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "oxid-esales/oxideshop-ce": "dev-b-7.0.x",
    "oxid-esales/oxideshop-unified-namespace-generator": "dev-b-7.0.x",
    "phpunit/phpunit": "~9",
    "webmozart/path-util": ">=2.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\Tests\\": "tests/"
    }
  }
}
