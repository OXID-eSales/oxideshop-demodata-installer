{
  "name": "oxid-esales/oxideshop-demodata-installer",
  "description": "This tool is used to copy pictures from demo data repository to OXID eShop, during setup process.",
  "license": "GPL-3.0",
  "require": {
    "php": ">=5.6",
    "symfony/filesystem": "^3.2",
    "oxid-esales/oxideshop-facts": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": { "OxidEsales\\DemoDataInstaller\\Tests\\": "tests/" }
  },
  "bin": [
    "./bin/oe-eshop-demodata_install"
  ]
}
