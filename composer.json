{
  "name": "oxid-esales/oxideshop-demodata-installer",
  "description": "This tool is used to copy pictures from demo data repository to OXID eShop, during setup process.",
  "license": "GPL-3.0-only",
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/filesystem": "^4.4.17",
    "oxid-esales/oxideshop-facts": "^2.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.14",
    "mikey179/vfsstream": "~1.6.8",
    "webmozart/path-util": ">=2.3"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "./bin/oe-eshop-demodata_install"
  ]
}
